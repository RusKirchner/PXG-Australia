      ],
      "settings": {
        "custom_liquid": "<div class=\"center\">\n<h2>\nBattle Ready II Brandon Specs\n</h2>\n</div>\n<div class=\"page-width sticky-first-column\">\n<table class=\"table-dark text-center table-alternating-rows\">\n  <tbody><tr>\n  <th scope=\"col\" class=\"table-head\">\n  Hosel Type\n  </th>\n  <th scope=\"col\">\n  Stock<br>Head Weight\n  </th>\n  <th scope=\"col\">\n  Minimum<br>Head Weight\n  </th>\n  <th scope=\"col\">\n  Maximum<br>Head Weight\n  </th>\n  <th scope=\"col\">\n  Loft\n  </th>\n  <th scope=\"col\">\n  Lie\n  </th>\n  <th scope=\"col\">\n  Hang Angle\n  </th>\n  <th scope=\"col\">\n  Offset\n  </th>\n  </tr>\n  <tr>\n  <th class=\"table-head\" scope=\"row\">Double Bend</th>\n  <td>340g</td>\n  <td>325g</td>\n  <td>360g</td>\n  <td>3°</td>\n  <td>70°</td>\n  <td>Face Balanced</td>\n  <td>Half Shaft</td>\n  </tr>\n  <tr>\n  <th class=\"table-head\" scope=\"row\">Heel Shafted</th>\n  <td>350g</td>\n  <td>335g</td>\n  <td>370g</td>\n  <td>3°</td>\n  <td>70°</td>\n  <td>Significant Toe Hang</td>\n  <td>Half Shaft</td>\n  </tr>\n  <tr>\n  <th class=\"table-head\" scope=\"row\">Plumber's Neck</th>\n  <td>360g</td>\n  <td>345g</td>\n  <td>380g</td>\n  <td>3°</td>\n  <td>70°</td>\n  <td>Mid Toe Hang</td>\n  <td>Full Shaft</td>\n  </tr>\n  <tr>\n  <th class=\"table-head\" scope=\"row\">Armlock</th>\n  <td>385g</td>\n  <td>370g</td>\n  <td>405g</td>\n  <td>7°</td>\n  <td>73°</td>\n  <td>Face Balanced</td>\n  <td>Full Shaft</td>\n  </tr>\n  </tbody></table></div>",
        "color_scheme": "background-1",
        "padding_top": 4,
        "padding_bottom": 52
      }
    },
